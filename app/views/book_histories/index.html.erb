<p id="notice"><%= notice %></p>

<h1>Book Histories</h1>

<table>
  <thead>
    <tr>
      <th>Book</th>
      <th>Issued To</th>
      <th>Issuedate</th>
      <th>Returndate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book_histories.each do |book_history| %>
      <tr>
        <td><% if book_history.book.present? %> book_history.book.ISBN<%else%><%end%></td>
        <td><% if book_history.student.present? %> book.student.id<%else%><%end%></td>
        <td><%= book_history.issueDate %></td>
        <td><%= book_history.returnDate %></td>
        <td><%= link_to 'Show', book_history %></td>
        <td><%= link_to 'Edit', edit_book_history_path(book_history) %></td>
        <td><%= link_to 'Destroy', book_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book History', new_book_history_path %>
